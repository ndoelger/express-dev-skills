<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Skills</title>
  </head>
  <body>
    <h1>Skills</h1>
    <ul>
      <% skills.forEach(function(s) { %>
      <li>
        Skill: <%= s.skill %> | Good?: <% if(s.good === true) { %> <%="Yes"%> <%
        } else { %> <%="No"%> <%}%>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
